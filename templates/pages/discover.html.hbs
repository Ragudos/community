{{#if is_boosted}}
    <head>
        {{> partials/head/main}}
        {{> partials/community/head}}
    </head>
    <aside id="community-header" hx-swap-oob="true"></aside>
    {{> partials/community/discover_ic}}
{{else}}
    {{#> layouts/main}}
        {{#*inline "additional_head_content"}}
            {{> partials/community/head}}
        {{/inline}}
        {{#*inline "header"}}
            <header id="header">
                {{> partials/headers/logged_in}}
            </header>
            <aside id="community-header"></aside>
        {{/inline}}
        {{> partials/community/discover_ic}}
    {{/layouts/main}}
{{/if}}