{{#if is_boosted}}
    <head>
        {{> partials/head/main}}
        <link rel="preload" href="/build/styles/discover.min.css" as="style">
        <link rel="stylesheet" href="/build/styles/discover.min.css">
    </head>
    <aside id="community-header" hx-swap-oob="true"></aside>
    {{> partials/discover/initial_state}}
{{else}}
    {{#> layouts/main}}
        {{#*inline "additional_head_content"}}
            <link rel="preload" href="/build/styles/discover.min.css" as="style">
            <link rel="stylesheet" href="/build/styles/discover.min.css">
        {{/inline}}
        {{#*inline "header"}}
            <header id="header">
                {{> partials/headers/logged_in}}
            </header>
            <aside id="community-header"></aside>
        {{/inline}}
        {{> partials/discover/initial_state}}
    {{/layouts/main}}
{{/if}}