{{#if is_boosted}}
    <head>
        {{> partials/head/main}}
    </head>
    {{#if isfromauth}}
        <header id="header" hx-swap-oob="true">
            {{> partials/headers/logged_in}}
        </header>
    {{/if}}
    <aside id="community-header" hx-swap-oob="true"></aside>
    {{> partials/discover/initial_state}}
{{else}}
    {{#> layouts/main}}
        {{#*inline "header"}}
            <header id="header">
                {{> partials/headers/logged_in}}
            </header>
            <aside id="community-header"></aside>
        {{/inline}}
        {{> partials/discover/initial_state}}
    {{/layouts/main}}
{{/if}}